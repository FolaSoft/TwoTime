<!DOCTYPE html>
<html>
  <head>
      <title>TwoTime Ruby on Rails App | <%= yield(:title) %></title>
	    <%= render 'layouts/stylesheet_link_tag' %>
	    <%= render 'layouts/javascript_include_tags' %>
	    <%= render 'layouts/shim' %>
  </head>
  
  <body>
      <%= render 'layouts/navbar' %>
       <div class="container">
          <% flash.each do |key, value| %>
             <div class="alert alert-<%= key %>"><%= value %></div>
          <% end %>
          <%= yield %>
          <%= debug(params) if Rails.env.development? %>
       <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
